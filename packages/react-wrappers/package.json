{
  "name": "custom-element-react-wrappers",
  "version": "1.7.0",
  "description": "A tool for generating react-compatible wrappers for custom elements",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "types.d.ts",
    "/dist"
  ],
  "repository": "https://github.com/break-stuff/cem-tools",
  "homepage": "https://github.com/break-stuff/cem-tools/tree/main/packages/react-wrappers#readme",
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "deploy": "npm run build && npm publish",
    "test": "vitest --run"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.6.3",
    "@types/node": "^18.6.3",
    "@types/prettier": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "custom-elements-manifest": "2.0.0",
    "eslint": "^8.32.0",
    "tsup": "^7.1.0"
  },
  "dependencies": {
    "@prettier/sync": "^0.5.2"
  },
  "keywords": [
    "custom-elements",
    "custom-elements-manifest",
    "web-components",
    "components",
    "react",
    "autocomplete",
    "intellisense"
  ],
  "author": "",
  "license": "MIT"
}
